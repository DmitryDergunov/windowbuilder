{
  "name": "windowbuilder",
  "version": "2.1.245",
  "description": "Order dealer for windows and stained glass",
  "repository": {
    "type": "git",
    "url": "https://github.com/oknosoft/windowbuilder.git"
  },
  "engines": {
    "node": "^8.8",
    "npm": "^6.0"
  },
  "keywords": [
    "metadata",
    "order dealer",
    "cad",
    "crm",
    "window builder"
  ],
  "author": "Evgeniy Malyarov <info@oknosoft.ru> (http://www.oknosoft.ru)",
  "license": "SEE LICENSE IN LICENSE.en.md",
  "private": true,
  "bugs": {
    "url": "https://github.com/oknosoft/windowbuilder/issues"
  },
  "homepage": "https://light.oknosoft.ru/",
  "proxy": "http://localhost:3030/",
  "dependencies": {
    "@jscad/openjscad": "^1.6.1",
    "dxf-writer": "^1.2.0",
    "genetic-cutting": "github:oknosoft/genetic-cutting",
    "metadata-abstract-ui": "2.0.19-beta.1",
    "metadata-core": "2.0.19-beta.1",
    "metadata-dhtmlx": "2.0.19-beta.1",
    "metadata-pouchdb": "2.0.19-beta.1",
    "metadata-react": "2.0.19-beta.1",
    "metadata-redux": "2.0.19-beta.1",
    "paper-jsdom-canvas": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "glob": "^7.1.3",
    "gulp": "^3.9",
    "gulp-base64": "^0.1",
    "gulp-concat": "^2.6",
    "gulp-csso": "^3.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-umd": "^2.0",
    "gulp-wrap": "^0.14",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "md5-file": "^4.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "raw-loader": "^2.0.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "files": [
    "public/dist/drawer.js",
    "public/dist/init.js",
    "LICENSE",
    "LICENSE.ru.md"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && node scripts/appcache.js",
    "test": "node scripts/test.js --env=jsdom",
    "meta:prebuild": "node scripts/meta.prebuild.js",
    "meta:link": "node scripts/meta.link.js",
    "rm-old-router": "rm ./node_modules/react-router-redux/node_modules/react-router"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
